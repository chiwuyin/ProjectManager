conda env export --no-builds > environment.yml

docker build -t my-ubuntu2204-conda .

docker build -t sound-separator-enh .

docker run --name sound-separator-enh-container -it sound-separator-enh

docker run --name sound-separator-enh-container -v E:/sound-separator-enh:/usr/src/app -it sound-separator-enh

docker run --name sound-separator-enh-container -v X:/vt:/ss/vt -it sound-separator-enh

docker run --name sound-separator-enh-container -v X:/vt:/ss/vt -it sound-separator-enh

docker exec -it sound-separator-enh-container /bin/bash
ls -la /ss/vt

